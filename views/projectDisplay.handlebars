<nav>
    <div class="nav-wrapper red">
        <a href="" class="brand-logo center">BootCamp Shortcuts</a>
        <div class="chip">{{{userName}}}</div>
        <div class="chip right">{{{projectName}}}</div>
    </div>
</nav>
<br>

<section class="container">
<div class="row">
  <div class="col s12">
    <ul class="collapsible popout">
      {{#each projects}}
        <li class="shortcutItem">
          <div class="collapsible-header">
            <i class="material-icons">code</i>
            {{this.title}}
            <i data-shortcutId="{{this.id}}" class="xButton close material-icons">close</i>
          </div>
          <div class="collapsible-body">
            <span>{{this.shortDescription}}</span>
            <br>
            <span>{{this.gettingStarted}}</span>
            <br>
            <span>{{this.links}}</span>
          </div>
        </li>
      {{/each}}
    </ul>
  </div>
</div>

</section>
